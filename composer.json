{
    "name": "tim-itwakes/plist",
    "description": "Library for reading and writing Apple's CFPropertyList (plist) files in XML as well as binary format.",
    "keywords": ["Core Foundation", "CFPropertyList", "plist", "apple", "xml", "binary"],
    "type": "library",
    "homepage": "https://github.com/tim-itwakes/CFPropertyList",
    "license": "MIT",
    "require": {
        "php": ">=5.6"
    },
    "authors": [
        {
            "name": "Tim",
            "email": "tim@itwakes.me",
            "role": "developer"
        }
    ],
    "support": {
        "issues": "https://github.com/tim-itwakes/CFPropertyList/issues",
        "source": "https://github.com/tim-itwakes/CFPropertyList"
    },
    "autoload": {
        "psr-4": {
            "CFPropertyList\\": "src/CFPropertyList/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0 || ^6.0 ||^7.0",
        "squizlabs/php_codesniffer": "^3.2"
    }
}
